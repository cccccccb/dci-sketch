{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "创建",
      "identifier": "dci.import.create.icon",
      "script": "import.js",
      "handler": "OnCreate",
      "shortcut": "command option shift n"
    },
    {
      "name": "画板颜色",
      "identifier": "dci.import.show.palette",
      "script": "import.js",
      "handler": "ShowLayerPalette",
      "shortcut": "command option shift c"
    },
    {
      "name": "重命名",
      "identifier": "dci.icon.rename",
      "script": "import.js",
      "handler": "OnRename",
      "shortcut": "command option shift r"
    },
    {
      "name": "画板间距",
      "identifier": "dci.icon.addpadding",
      "script": "import.js",
      "handler": "AddArtboardPadding",
      "shortcut": "command option shift p"
    },
    {
      "name": "打开",
      "identifier": "dci.import.open",
      "script": "import.js",
      "handler": "OnOpen",
      "shortcut": "command option shift o"
    },
    {
      "name": "导出图标",
      "identifier": "dci.export.icon",
      "script": "export.js",
      "handler": "OnExportIcon",
      "shortcut": "command option shift e"
    },
    {
      "name": "导出整页",
      "identifier": "dci.export.page",
      "script": "export.js",
      "handler": "OnExportPage"
    },
    {
      "name": "导出全部",
      "identifier": "dci.export.all",
      "script": "export.js",
      "handler": "OnExportAll"
    }
  ],
  "menu": {
    "title": "DCI",
    "items": [
      "dci.import.create.icon",
      "dci.import.show.palette",
      "dci.icon.addpadding",
      "dci.import.open",
      "dci.icon.rename",
      "dci.export.icon",
      "dci.export.page",
      "dci.export.all"
    ]
  }
}
